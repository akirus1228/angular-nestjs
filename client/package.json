{
  "name": "angify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:ssl": "ng serve --ssl --ssl-key ../../localhost-key.pem  --ssl-cert ../../localhost.pem --open",
    "start": "ng serve --open",
    "build": "ng build --watch",
    "build:prod": "ng build --prod",
    "build:prod:cordova": "ng build --prod --base-href . --output-path ./CordovaMobileApp/www/",
    "build:dev:cordova": "ng build  --base-href . --output-path ./CordovaMobileApp/www/ --watch",
    "cordova:init": "cordova create CordovaMobileApp com.yantrab.app CordovaMobileApp",
    "cordova:build": "npm run cordova:init&&npm run build:prod:cordova",
    "cordova:run:browser": "cd CordovaMobileApp&&cordova platform add browser&&cordova run browser",
    "cordiva:add:android": "cd CordovaMobileApp&&cordova platform add android",
    "cordova:run:android": "cd CordovaMobileApp&&cordova plugin add cordovarduino&&cordova run android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "flogin": "firebase login",
    "fdeploy": "firebase deploy",
    "deploy": "npm run build:prod && npm run fdeploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0-rc.4",
    "@angular/cdk": "^8.0.0-rc.1",
    "@angular/common": "^8.0.0-rc.4",
    "@angular/compiler": "^8.0.0-rc.4",
    "@angular/core": "^8.0.0-rc.4",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^8.0.0-rc.4",
    "@angular/material": "^8.0.0-rc.1",
    "@angular/platform-browser": "~8.0.0-rc.4",
    "@angular/platform-browser-dynamic": "~8.0.0-rc.4",
    "@angular/router": "~8.0.0-rc.4",
    "devextreme": "^18.2.7",
    "devextreme-angular": "^18.2.7",
    "file-saver": "^2.0.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "mat-virtual-table": "^1.4.8",
    "ng-dyna-form": "0.0.7",
    "ngx-socket-io": "^2.1.1",
    "rxjs": "~6.4.0",
    "saturn-datepicker": "^7.2.1",
    "tslib": "^1.9.0",
    "webrtc-adapter": "^7.2.1",
    "xlsx": "^0.14.2",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-rc.2",
    "@angular/cli": "~8.0.0-rc.3",
    "@angular/compiler-cli": "~8.0.0-rc.2",
    "@angular/language-service": "~8.0.0-rc.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.17.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
