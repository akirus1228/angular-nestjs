{
    "name": "angify",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:ssl": "ng serve  -c dev --ssl --ssl-key ../../localhost-key.pem  --ssl-cert ../../localhost.pem --open",
        "start": "ng serve --open   -c dev",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "build:dev": "ng build  -c dev",
        "build:prod:cordova": "ng build --prod --base-href . --output-path ./CordovaMobileApp/www/",
        "build:dev:cordova": "ng build  --base-href . --output-path ./CordovaMobileApp/www/ --watch",
        "cordova:init": "cordova create CordovaMobileApp com.yantrab.app CordovaMobileApp",
        "cordova:run:browser": "cd CordovaMobileApp&&cordova platform add browser&&cordova run browser",
        "cordiva:add:android": "cd CordovaMobileApp&&cordova platform add android",
        "cordova:run:android": "cd CordovaMobileApp&&cordova plugin add cordovarduino&&cordova run android",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "flogin": "firebase login",
        "fdeploy": "firebase deploy",
        "deploy-dev": "npm run build:dev && npm run fdeploy",
        "deploy-prod": "npm run build:prod && npm run fdeploy"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^9.0.0-rc.8",
        "@angular/cdk": "^9.0.0-rc.7",
        "@angular/common": "~9.0.0-rc.8",
        "@angular/compiler": "~9.0.0-rc.8",
        "@angular/core": "~9.0.0-rc.8",
        "@angular/flex-layout": "^8.0.0-beta.27",
        "@angular/forms": "~9.0.0-rc.8",
        "@angular/material": "^9.0.0-rc.7",
        "@angular/platform-browser": "~9.0.0-rc.8",
        "@angular/platform-browser-dynamic": "~9.0.0-rc.8",
        "@angular/router": "~9.0.0-rc.8",
        "angular-svg-icon": "^8.0.0",
        "angular2-websocket": "^0.9.8",
        "date-fns": "^2.2.1",
        "devextreme": "^19.1.7",
        "devextreme-angular": "^19.1.7",
        "file-saver": "^2.0.2",
        "hammerjs": "^2.0.8",
        "lodash": "^4.17.15",
        "luxon": "^1.16.0",
        "mat-virtual-table": "^9.0.2",
        "ng-dialog-animation": "^1.0.0",
        "ng-dyna-form": "^9.0.1",
        "ngx-material-timepicker": "4.0.2",
        "ngx-socket-io": "^3.0.1",
        "rxjs": "~6.5.3",
        "saturn-datepicker": "^8.0.1",
        "socket.io-client": "^2.3.0",
        "tslib": "^1.10.0",
        "xlsx": "^0.15.1",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.0-rc.0",
        "@angular/cli": "~9.0.0-rc.0",
        "@angular/compiler-cli": "~9.0.0-rc.0",
        "@angular/language-service": "~9.0.0-rc.0",
        "@types/jasmine": "~3.4.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/lodash": "^4.14.144",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "~5.4.2",
        "ts-node": "~8.3.0",
        "tslint": "~5.18.0",
        "typescript": "3.6.4",
        "@angular-builders/custom-webpack": "^8.2.0"
    }
