{
    "name": "angify",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:ssl": "ng serve  -c dev --ssl --ssl-key ../../localhost-key.pem  --ssl-cert ../../localhost.pem --open",
        "start": "ng serve --open   -c dev",
        "build": "ng build --watch",
        "build:prod": "ng build --prod",
        "build:dev": "ng build --configuration=dev",
        "build:prod:cordova": "ng build --prod --base-href . --output-path ./CordovaMobileApp/www/",
        "build:dev:cordova": "ng build  --base-href . --output-path ./CordovaMobileApp/www/ --watch",
        "cordova:init": "cordova create CordovaMobileApp com.yantrab.app CordovaMobileApp",
        "cordova:build": "npm run cordova:init&&npm run build:prod:cordova",
        "cordova:run:browser": "cd CordovaMobileApp&&cordova platform add browser&&cordova run browser",
        "cordiva:add:android": "cd CordovaMobileApp&&cordova platform add android",
        "cordova:run:android": "cd CordovaMobileApp&&cordova plugin add cordovarduino&&cordova run android",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "flogin": "firebase login",
        "fdeploy": "firebase deploy",
        "deploy-dev": "npm run build:dev && npm run fdeploy",
        "deploy-prod": "npm run build:prod && npm run fdeploy"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^8.0.1",
        "@angular/cdk": "^8.0.1",
        "@angular/common": "^8.0.1",
        "@angular/compiler": "^8.0.1",
        "@angular/core": "^8.0.1",
        "@angular/flex-layout": "8.0.0-beta.26",
        "@angular/forms": "^8.0.1",
        "@angular/material": "^8.0.1",
        "@angular/material-moment-adapter": "^8.0.1",
        "@angular/platform-browser": "^8.0.1",
        "@angular/platform-browser-dynamic": "^8.0.1",
        "@angular/router": "^8.0.1",
        "date-fns": "latest",
        "devextreme": "^18.2.7",
        "devextreme-angular": "^18.2.7",
        "file-saver": "^2.0.1",
        "hammerjs": "^2.0.8",
        "lodash": "^4.17.11",
        "luxon": "^1.16.0",
        "mat-virtual-table": "^1.5.2",
        "ng-dyna-form": "0.0.7",
        "ngx-material-timepicker": "^3.1.0",
        "ngx-socket-io": "^2.1.1",
        "rxjs": "~6.4.0",
        "saturn-datepicker": "^7.2.1",
        "tslib": "^1.9.0",
        "webrtc-adapter": "^7.2.1",
        "xlsx": "^0.14.2",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.800.0",
        "@angular-devkit/core": "^8.0.2",
        "@angular-devkit/schematics": "^8.0.2",
        "@angular/cli": "~8.0.3",
        "@angular/compiler-cli": "~8.0.1",
        "@angular/language-service": "~8.0.0",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/moment": "^2.13.0",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "prettier": "^1.17.1",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.15.0",
        "typescript": "~3.4.3",
        "webpack-dev-server": "^3.7.1"
    }
}
